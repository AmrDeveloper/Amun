extern fun printf(format *char, value int64) int64;

fun main() int64 {
    var x : int64 = 10;
    x += 1;
    printf("x += 1 %i expect 11", x);

    x -= 1;
    printf("x -= 1 %i\n expect 10", x);

    x *= 10;
    printf("x *= 1 %i\n expect 100", x);

    x /= 10;
    printf("x /= 1 %i\n expect 10", x);

    var y = 10;
    x += y;
    printf("x += y %i\n expect 20", x);
    return 0;
}