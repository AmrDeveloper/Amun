@extern fun printf(format *char, varargs Any) int64;

fun main() int64 {
    printf("Int64 size %d Byte\n", type_allign(int64));
    printf("Int32 size %d Byte\n", type_allign(int32));
    printf("Int16 size %d Byte\n", type_allign(int16));
    printf("Int8 size %d Byte\n", type_allign(int8));
    printf("Char size %d Byte\n", type_allign(char));

    printf("Float64 size %d Byte\n", type_allign(float64));
    printf("Float32 size %d Byte\n", type_allign(float32));
    return 0;
}