extern fun puts(s *char) int64;

fun main(argc int64) int64 {
    var i = 10;
    while (i > 0) {
        i -= 1;
        var j = 3;
        while (j > 0) {
            // This instruction will continue the first while loop
            // Expected intput to be empty
            continue 2;
            j -= 1;
            puts("Hello");
        }
        puts("World");
    }
    return 0;
}