@extern fun printf(format *char, value int64) int64;

fun main() int64 {
    var x = 10;
    switch (x) {
        1 -> printf("Value is %d\n", x);
        10 -> printf("Value is %d\n", x);
        else -> printf("Default branch %d\n", x);
    }

    printf("X After switch Value is %d\n", x);
    return 0;
}